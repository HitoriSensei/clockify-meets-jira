    "build": "tsc",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^4.15.2",
    "http-auth-utils": "^2.6.0",
